{
  "name": "apextime-backend",
  "version": "1.0.0",
  "description": "ApexTime Attendance Management System Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "seed": "ts-node src/utils/seed.ts",
    "sync": "ts-node src/services/logSyncService.ts",
    "sync-logs": "ts-node src/services/logSyncService.ts",
    "sync:full": "ts-node src/services/logSyncService.ts full-sync",
    "sync:reprocess": "ts-node src/services/logSyncService.ts reprocess",
    "reprocess": "ts-node src/services/logSyncService.ts reprocess"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/node-fetch": "^2.6.13",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^3.0.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^10.0.1",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.14.0",
    "winston": "^3.11.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mssql": "^9.1.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.13.3",
    "@types/ws": "^8.18.1",
    "node-fetch": "2",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "attendance",
    "biometric",
    "hr",
    "management"
  ],
  "author": "ApexTime",
  "license": "MIT"
}
